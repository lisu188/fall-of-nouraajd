{
  "characterPanel": {
    "class": "CGameCharacterPanel",
    "properties": {
      "charSheet": {
        "class": "CListString",
        "properties": {
          "values": [
            "level",
            "exp",
            "gold",
            "mana",
            "manaMax",
            "manaRegRate",
            "hp",
            "hpMax"
          ]
        }
      },
      "layout": {
        "class": "CCenteredLayout",
        "properties": {
          "h": 600,
          "w": 800
        }
      }
    }
  },
  "creatureView": {
    "class": "CCreatureView",
    "properties": {
      "children": [
        {
          "class": "CListView",
          "properties": {
            "allowOversize": false,
            "collection": "getEffects",
            "layout": {
              "class": "CAnchoredLayout",
              "properties": {
                "horizontal": "LEFT",
                "vertical": "PARENT",
                "w": 50
              }
            },
            "priority": 1,
            "refreshEvent": "effectsChanged",
            "refreshObject": {
              "class": "CScript",
              "properties": {
                "script": "self.getParent().getCreature()"
              }
            },
            "showEmpty": false,
            "tileSize": 25,
            "xPrefferedSize": 1,
            "yPrefferedSize": 4
          }
        }
      ],
      "creatureScript": {
        "class": "CScript",
        "properties": {
          "script": "self.getParent().getParent().getEnemy()"
        }
      },
      "layout": {
        "class": "CAnchoredLayout",
        "properties": {
          "h": 200,
          "horizontal": "PARENT",
          "vertical": "UP"
        }
      },
      "proxyLayout": "CParentLayout"
    }
  },
  "dialogPanel": {
    "class": "CGameDialogPanel",
    "properties": {
      "children": [
        {
          "class": "CWidget",
          "properties": {
            "layout": {
              "class": "CPercentLayout",
              "properties": {
                "h": 75,
                "w": 100,
                "x": 0,
                "y": 0
              }
            },
            "render": "renderQuestion"
          }
        },
        {
          "class": "CTextWidget",
          "properties": {
            "click": "clickYes",
            "layout": {
              "class": "CPercentLayout",
              "properties": {
                "h": 25,
                "w": 50,
                "x": 0,
                "y": 75
              }
            },
            "text": "YES"
          }
        },
        {
          "class": "CTextWidget",
          "properties": {
            "click": "clickNo",
            "layout": {
              "class": "CPercentLayout",
              "properties": {
                "h": 25,
                "w": 50,
                "x": 50,
                "y": 75
              }
            },
            "text": "NO"
          }
        }
      ],
      "layout": {
        "class": "CCenteredLayout",
        "properties": {
          "h": 300,
          "w": 400
        }
      }
    }
  },
  "fightPanel": {
    "class": "CGameFightPanel",
    "properties": {
      "children": [
        {
          "class": "CGameGraphicsObject",

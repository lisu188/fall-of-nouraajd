{
  "characterPanel": {
    "class": "CGameCharacterPanel",
    "properties": {
      "charSheet": {
        "class": "CListString",
        "properties": {
          "values": [
            "level",
            "exp",
            "gold",
            "mana",
            "manaMax",
            "manaRegRate",
            "hp",
            "hpMax"
          ]
        }
      },
      "layout": {
        "class": "CCenteredLayout",
        "properties": {
          "h": 600,
          "w": 800
        }
      }
    }
  },
  "dialogPanel": {
    "class": "CGameDialogPanel",
    "properties": {
      "children": [
        {
          "class": "CWidget",
          "properties": {
            "layout": {
              "class": "CPercentLayout",
              "properties": {
                "h": 75,
                "w": 100,
                "x": 0,
                "y": 0
              }
            },
            "render": "renderQuestion"
          }
        },
        {
          "class": "CWidget",
          "properties": {
            "click": "clickYes",
            "layout": {
              "class": "CPercentLayout",
              "properties": {
                "h": 25,
                "w": 50,
                "x": 0,
                "y": 75
              }
            },
            "render": "renderYes"
          }
        },
        {
          "class": "CWidget",
          "properties": {
            "click": "clickNo",
            "layout": {
              "class": "CPercentLayout",
              "properties": {
                "h": 25,
                "w": 50,
                "x": 50,
                "y": 75
              }
            },
            "render": "renderNo"
          }
        }
      ],
      "layout": {
        "class": "CCenteredLayout",
        "properties": {
          "h": 300,
          "w": 400
        }
      }
    }
  },
  "fightPanel": {
    "class": "CGameFightPanel",
    "properties": {
      "children": [
        {
          "class": "CGameGraphicsObject",
          "properties": {
            "children": [
              {
                "class": "CStatsGraphicsObject",
                "properties": {
                  "creature": {
                    "class": "CScript",
                    "properties": {
                      "script": "self.getParent().getParent().getEnemy()"
                    }
                  },
                  "layout": {
                    "class": "CAnchoredLayout",
                    "properties": {
                      "h": 100,
                      "horizontal": "PARENT",
                      "vertical": "DOWN"
                    }
                  }
                }
              },
              {
                "class": "CCreatureView",
                "properties": {
                  "creature": {
                    "class": "CScript",
                    "properties": {
                      "script": "self.getParent().getParent().getEnemy()"
                    }
                  },
                  "proxyLayout": "CParentLayout",
                  "layout": {
                    "class": "CAnchoredLayout",
                    "properties": {
                      "h": 200,
                      "horizontal": "PARENT",
                      "vertical": "UP"
                    }
                  }
                }
              }
            ],
            "layout": {
              "class": "CSimpleLayout",
              "properties": {
                "h": 300,
                "w": 200,
                "x": 300,
                "y": 100
              }
            }
          }
        },
        {
          "class": "CListView",
          "properties": {
            "collection": "interactionsCollection",
            "callback": "interactionsCallback",
            "select": "interactionsSelect",
            "refreshObject": {
              "class": "CScript",
              "properties": {
                "script": "self.getGui().getGame().getMap().getPlayer()"
              }
            },
            "refreshEvent": "interactionsChanged",
            "layout": {
              "class": "CPercentLayout",
              "properties": {
                "h": 10,
                "w": 100,
                "x": 0,
                "y": 90
              }
            }
          }
        },
        {
          "class": "CListView",
          "properties": {
            "collection": "itemsCollection",
            "callback": "itemsCallback",
            "select": "itemsSelect",
            "refreshObject": {
              "class": "CScript",
              "properties": {
                "script": "self.getGui().getGame().getMap().getPlayer()"
              }
            },
            "refreshEvent": "inventoryChanged",
            "layout": {
              "class": "CPercentLayout",
              "properties": {
                "h": 10,
                "w": 100,
                "x": 0,
                "y": 80
              }
            }
          }
        }
      ],
      "layout": {
        "class": "CCenteredLayout",
        "properties": {
          "h": 600,
          "w": 800
        }
      }
    }
  },

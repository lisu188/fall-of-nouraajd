                "ref": "creatureView"
              },
              {
                "properties": {
                  "creature": {
                    "class": "CScript",
                    "properties": {
                      "script": "self.getGui().getGame().getMap().getPlayer()"
                    }
                  }
                },
                "ref": "statsView"
              }
            ],
            "layout": {
              "class": "CSimpleLayout",
              "properties": {
                "h": 300,
                "w": 200,
                "x": 150,
                "y": 100
              }
            }
          }
        },
        {
          "class": "CGameGraphicsObject",
          "properties": {
            "children": [
              {
                "properties": {
                  "creatureScript": {
                    "class": "CScript",
                    "properties": {
                      "script": "self.getParent().getParent().getEnemy()"

            "layout": {
              "class": "CPercentLayout",
              "properties": {
                "h": 10,
                "w": 100,
                "x": 0,
                "y": 90
              }
            },
            "refreshEvent": "interactionsChanged",
            "refreshObject": {
              "class": "CScript",
              "properties": {
                "script": "self.getGui().getGame().getMap().getPlayer()"
              }
            },
            "select": "interactionsSelect"
          }
        },
        {
          "class": "CListView",
          "properties": {
            "callback": "itemsCallback",
            "collection": "itemsCollection",
            "layout": {
              "class": "CPercentLayout",
              "properties": {
                "h": 10,

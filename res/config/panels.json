{
  "characterPanel": {
    "class": "CGameCharacterPanel",
    "properties": {
      "children": [
        {
          "class": "CListView",
          "properties": {
            "collection": "interactionsCollection",
            "layout": {
              "class": "CLayout",
              "properties": {
                "h": "10%",
                "w": "100%",
                "x": "0%",
                "y": "90%"
              }
            },
            "refreshEvent": "interactionsChanged",
            "refreshObject": {
              "class": "CScript",
              "properties": {
                "script": "self.getGui().getGame().getMap().getPlayer()"
              }
            }
          }
        }
      ],
      "charSheet": {
        "class": "CMapStringString",
        "properties": {
          "values": {
            "Level": "getLevel",
            "Experience": "getExp",
            "Gold": "getGold",
            "Mana": "getMana",
            "Total Mana": "getManaMax",
            "Mana Regeneration Rate": "getManaRegRate",
            "Health Points": "getHp",
            "Total Health Points": "getHpMax"
          }
        }
      },
      "layout": {
        "class": "CCenteredLayout",
        "properties": {
          "h": "600",
          "w": "800"
        }
      }
    }
  },
  "creatureView": {
    "class": "CCreatureView",
    "properties": {
      "children": [
        {
          "class": "CListView",
          "properties": {
            "allowOversize": false,
            "collection": "getEffects",
            "layout": {
              "class": "CLayout",
              "properties": {
                "horizontal": "LEFT",
                "vertical": "PARENT",
                "w": 50
              }
            },
            "priority": 1,
            "refreshEvent": "effectsChanged",
            "refreshObject": {
              "class": "CScript",
              "properties": {
                "script": "self.getParent().getCreature()"
              }
            },
            "showEmpty": false,
            "tileSize": 25,
            "xPrefferedSize": 1,
            "yPrefferedSize": 4
          }
        }
      ],
      "creatureScript": {
        "class": "CScript",
        "properties": {
          "script": "self.getParent().getParent().getEnemy()"
        }
      },
      "layout": {
        "class": "CLayout",
        "properties": {
          "h": 200,
          "horizontal": "PARENT",
          "vertical": "UP"
        }
      },
      "proxyLayout": "CParentLayout"
    }
  },
  "questionPanel": {
    "class": "CGameQuestionPanel",
    "properties": {
      "children": [
        {
          "class": "CWidget",

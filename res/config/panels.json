{
  "characterPanel": {
    "class": "CGameCharacterPanel",
    "properties": {
      "layout": {
        "charSheet": {
          "class": "CListString",
          "properties": {
            "values": [
              "level",
              "exp",
              "gold",
              "mana",
              "manaMax",
              "manaRegRate",
              "hp",
              "hpMax"
            ]
          }
        },
        "class": "CCenteredLayout",
        "properties": {
          "height": 600,
          "width": 800
        }
      }
    }
  },
  "dialogPanel": {
    "class": "CGameDialogPanel",
    "properties": {
      "children": [
        {
          "class": "CWidget",
          "properties": {
            "layout": {
              "class": "CPercentLayout",
              "properties": {
                "h": 75,
                "w": 100,
                "x": 0,
                "y": 0
              }
            },
            "render": "renderQuestion"
          }
